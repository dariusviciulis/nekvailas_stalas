<head>
    {% block head %}
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>{% block title %}table status{% endblock %}</title>
        <meta name="description" content="{% block description %}{% endblock %}">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="http://{{ app.request.host }}/kickertable/web/css/normalize.css">
        <link rel="stylesheet" href="http://{{ app.request.host }}/kickertable/web/css/main.css">
    {% endblock %}
</head>
<body>

    <div id="wrap">
        {% block content %}{% endblock %}
    </div>

    <script src="http://{{ app.request.host }}/kickertable/web/js/vendor/jquery-1.10.2.min.js"></script>
    <script src="http://{{ app.request.host }}/kickertable/web/js/vendor/modernizr-2.6.2.min.js"></script>
    {% block javascript %}{% endblock %}

</body>
</html>